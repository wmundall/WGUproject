<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="787.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomersViewController">
    <children>
        <TableView fx:id="customerTable" layoutX="11.0" layoutY="82.0" prefHeight="200.0" prefWidth="604.0">
            <columns>
                <TableColumn fx:id="customerIDCol" prefWidth="75.0" text="Customer ID" />
                <TableColumn fx:id="nameCol" prefWidth="111.0" text="Name" />
                <TableColumn fx:id="addressCol" prefWidth="97.0" text="Address" />
                <TableColumn fx:id="postalCodeCol" prefWidth="75.0" text="Postal Code" />
                <TableColumn fx:id="phoneCol" prefWidth="131.0" text="Phone" />
                <TableColumn fx:id="divisionIDCol" prefWidth="67.0" text="Division ID" />
            </columns>
        </TableView>
        <Button layoutX="604.0" layoutY="350.0" mnemonicParsing="false" onAction="#BackToCustomers" text="Back to Customers Main" />
      <Button layoutX="14.0" layoutY="308.0" mnemonicParsing="false" onAction="#GoToCustomerModify" text="Update Customer" />
      <Button layoutX="660.0" layoutY="22.0" mnemonicParsing="false" onAction="#goToSpecialQueryWindow" prefHeight="25.0" prefWidth="102.0" text="Custom Reports" />
      <Label fx:id="errorLbl" layoutX="180.0" layoutY="312.0" prefHeight="17.0" prefWidth="344.0" text="Label" textFill="#d31a1a" visible="false" />
    </children>
</AnchorPane>
