<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerAddController">
   <children>
      <TableView fx:id="customerTable" layoutX="10.0" layoutY="62.0" prefHeight="200.0" prefWidth="754.0">
        <columns>
          <TableColumn fx:id="ID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="name" prefWidth="74.0" text="Name" />
            <TableColumn fx:id="address" prefWidth="76.0" text="Address" />
            <TableColumn fx:id="postalCode" prefWidth="75.0" text="Postal Code" />
            <TableColumn fx:id="phone" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="createDate" prefWidth="75.0" text="Create Date" />
            <TableColumn fx:id="createdBy" prefWidth="75.0" text="Created By" />
            <TableColumn fx:id="lastUpdate" prefWidth="75.0" text="Last Update" />
            <TableColumn fx:id="lastUpdatedBy" prefWidth="71.0" text="Last Updated By" />
            <TableColumn fx:id="divisionID" prefWidth="79.0" text="Division ID" />
        </columns>
      </TableView>
      <Button layoutX="47.0" layoutY="309.0" mnemonicParsing="false" onAction="#GoBack" text="Back" />
      <ComboBox fx:id="countryCombo" layoutX="369.0" layoutY="284.0" onMouseClicked="#SelectCountry" prefWidth="150.0" promptText="pick country" />
      <ComboBox layoutX="548.0" layoutY="284.0" prefWidth="150.0" promptText="pick 1st level division" />
      <TextField fx:id="textfield" layoutX="132.0" layoutY="347.0" />
   </children>
</AnchorPane>
